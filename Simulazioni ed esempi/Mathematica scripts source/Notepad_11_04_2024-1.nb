      RowBox[{
       RowBox[{
        RowBox[{"-", "3"}], " ", 
        RowBox[{"Cos", "[", 
         FractionBox[
          RowBox[{"k", " ", "\[Pi]"}], "3"], "]"}]}], "+", 
