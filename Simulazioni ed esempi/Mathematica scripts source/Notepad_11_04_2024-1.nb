Cell["\<\
Scrivo ora la risposta libera sfruttando la forma canonica Rotation-Scaling\
\>", "Text",
 CellChangeTimes->{{3.921815376787697*^9, 
  3.921815396528857*^9}},ExpressionUUID->"37d11d90-51da-4c30-b1d9-\
8520625070ae"],

Cell[BoxData[
 RowBox[{
  RowBox[{"x", "[", "k_", "]"}], ":=", 
  RowBox[{"Simplify", "[", 
   RowBox[{
    OverscriptBox["T", "^"], ".", 
    RowBox[{"(", "\[NoBreak]", GridBox[{
       {
        RowBox[{
         SuperscriptBox["\[Rho]", "k"], 
         RowBox[{"Cos", "[", 
          RowBox[{"\[Theta]", " ", "k"}], "]"}]}], 
        RowBox[{
         SuperscriptBox["\[Rho]", "k"], 
         RowBox[{"Sin", "[", 
          RowBox[{"\[Theta]", " ", "k"}], "]"}]}]},
       {
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox["\[Rho]", "k"]}], 
         RowBox[{"Sin", "[", 
          RowBox[{"\[Theta]", " ", "k"}], "]"}]}], 
        RowBox[{
         SuperscriptBox["\[Rho]", "k"], 
         RowBox[{"Cos", "[", 
          RowBox[{"\[Theta]", " ", "k"}], "]"}]}]}
      }], "\[NoBreak]", ")"}], ".", 
    SubscriptBox["z", "0"]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.921815400832045*^9, 3.921815409725532*^9}, {
  3.921815448674357*^9, 3.921815525721636*^9}},
 CellLabel->"In[26]:=",ExpressionUUID->"8cbec56f-c085-4733-94a5-6a23a0879c39"],

