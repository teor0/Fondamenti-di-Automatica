Cell[BoxData[
 RowBox[{
  RowBox[{"MatrixMinimalPolynomial", "[", 
   RowBox[{
    RowBox[{"a_List", "?", "MatrixQ"}], ",", "x_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i", ",", 
      RowBox[{"n", "=", "1"}], ",", 
      RowBox[{"qu", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"mnm", "=", 
