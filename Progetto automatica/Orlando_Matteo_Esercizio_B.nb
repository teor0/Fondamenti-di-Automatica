          RowBox[{"Length", "[", "a", "]"}], "]"}], "]"}], "}"}]}]}], "}"}], 
    ",", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "qu", "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"mnm", ",", 
