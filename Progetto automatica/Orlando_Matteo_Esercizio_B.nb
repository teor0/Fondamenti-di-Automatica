            RowBox[{"a", ",", "n"}], "]"}], "]"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"qu", "=", 
         RowBox[{"NullSpace", "[", 
          RowBox[{"Transpose", "[", "mnm", "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"n", "++"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"First", "[", "qu", "]"}], ".", 
